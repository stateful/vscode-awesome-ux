{
	"name": "awesome-ux",
	"displayName": "Awesome UX",
	"description": "Reference implementation of an VSCode extension with an awesome UX",
	"version": "0.3.3",
	"license": "MIT",
	"publisher": "stateful",
	"icon": ".github/assets/stateful.png",
	"galleryBanner": {
		"color": "#2F2F2F",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/stateful/vscode-awesome-ux.git"
	},
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"vscode",
		"extension",
		"best practices"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"browser": "./out/web.js",
	"main": "./out/index.js",
	"extensionKind": [
		"ui"
	],
	"contributes": {
		"configuration": [
			{
				"title": "Awesome Extension",
				"properties": {
					"vscode-awesome-ux.configuration.defaultNotifications": {
						"type": "number",
						"scope": "resource",
						"default": 0,
						"markdownDescription": "The default value of received example notification (default `0`)"
					}
				}
			}
		],
		"commands": [
			{
				"command": "tangle.emit",
				"title": "Ring Bell!"
			},
			{
				"command": "vscode-awesome-ux.getController",
				"title": "_"
			}
		],
		"views": {
			"explorer": [
				{
					"id": "panel1",
					"name": "Example Panel 1",
					"type": "webview",
					"visibility": "visible"
				},
				{
					"id": "panel2",
					"name": "Example Panel 2",
					"type": "webview",
					"visibility": "visible"
				}
			]
		}
	},
	"scripts": {
		"build": "run-s clean compile",
		"clean": "rimraf out",
		"compile": "rollup -c ./rollup.config.mjs",
		"test": "run-s test:*",
		"test:lint": "eslint src --ext ts",
		"test:e2e": "cd test && npx wdio run ./wdio.conf.ts",
		"watch": "npm run clean && npm run compile -- --watch"
	},
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@rollup/plugin-typescript": "^8.3.3",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "18.x",
		"@types/vscode": "^1.69.0",
		"@types/vscode-webview": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^5.30.6",
		"@typescript-eslint/parser": "^5.30.6",
		"@vscode/test-electron": "^2.1.5",
		"@wdio/cli": "^7.20.5",
		"@wdio/local-runner": "^7.20.5",
		"@wdio/mocha-framework": "^7.20.3",
		"@wdio/spec-reporter": "^7.20.3",
		"chromedriver": "103",
		"eslint": "^8.19.0",
		"eta": "^1.12.3",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"rollup": "^2.76.0",
		"rollup-plugin-eta": "^1.0.0",
		"rollup-plugin-node-polyfills": "^0.2.1",
		"ts-node": "^10.8.2",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"vsce": "^2.9.2",
		"wdio-vscode-service": "^4.0.0",
		"webdriverio": "^7.20.5"
	},
	"dependencies": {
		"@vscode/codicons": "^0.0.31",
		"@vscode/webview-ui-toolkit": "^1.0.0",
		"lit-element": "^3.2.1",
		"rollup-plugin-multi-input": "^1.3.1",
		"rxjs": "^7.5.6",
		"tangle": "^2.1.0"
	},
	"__metadata": {
		"id": "fdeb35df-57e6-40a7-bd49-335f989bac33",
		"publisherDisplayName": "stateful",
		"publisherId": "e01ecf81-8aec-4229-a7c4-570a9fa31ca5",
		"isPreReleaseVersion": false
	}
}
