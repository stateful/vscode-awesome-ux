{
	"name": "vscode-awesome-ux",
	"displayName": "vscode-awesome-ux",
	"description": "Reference implementation of vscode extension with awesome UX",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscoderx.emit",
				"title": "VS Code RX Emit"
			}
		],
		"views": {
			"explorer": [
				{
					"id": "panel-one",
					"name": "VRX 1",
					"type": "webview",
					"visibility": "visible"
				},
				{
					"id": "panel-two",
					"name": "VRX 2",
					"type": "webview",
					"visibility": "visible"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p -w . --outDir out",
		"pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.61.0",
		"@typescript-eslint/eslint-plugin": "^5.2.0",
		"@typescript-eslint/parser": "^5.2.0",
		"@vscode/test-electron": "^1.6.2",
		"copy-webpack-plugin": "^9.0.1",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"ts-loader": "^9.2.5",
		"typescript": "^4.4.3",
		"webpack": "^5.60.0",
		"webpack-cli": "^4.8.0"
	},
	"dependencies": {
		"rxjs": "^7.4.0"
	}
}